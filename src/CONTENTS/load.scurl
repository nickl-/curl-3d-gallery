{curl 6.0 package}
{curl-file-attributes character-encoding = "shift-jis"}

{package CONTENTS,
    {compiler-directives
        careful? = true
    },
    resource-directory = "../resources",
    translations-file = "messages.xml"
}

{import * from CURL.GUI.BASE}
{import * from CURL.GRAPHICS.SCENE}
{import * from CURL.GUI.DIALOGS}
{import * from CURL.GUI.CONTROLS}
{import * from CURL.GUI.CONTROL-BASE}
{import * from CURL.ENGINE.APPLET}
{import * from CURL.GUI.STANDARD}

{import * from SCENE}
{import * from UTIL}
{import * from MODEL}
{import * from ELEMENT}
{import * from CONTROL}

{include
    "Contents.scurl",
    "Menu.scurl",
    "ControlPanel.scurl",
    "CameraParameterDialog.scurl",
    "LinkInfoEditDialog.scurl",
    "LinkInfoListDialog.scurl"
}

{doc
    type = package,
    CONTENTS
    {purpose
        このパッケージは、メイン画面にかかわる情報を提供します。
    }
    {notes
        追加のダイアログは、このパッケージに追加していきます。
    }
}
